{
  "name": "friendsofredaxo/rexql",
  "description": "GraphQL API for REDAXO CMS",
  "type": "redaxo-addon",
  "license": "MIT",
  "authors": [
    {
      "name": "REDAXO Community",
      "homepage": "https://redaxo.org"
    }
  ],
  "require": {
    "php": "^8.1",
    "webonyx/graphql-php": "^15.0",
    "symfony/cache": "^5.4"
  },
  "replace": {
    "psr/log": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "FriendsOfRedaxo\\RexQL\\": "lib/"
    }
  },
  "extra": {
    "redaxo": {
      "package-dir": "src/addons/rexql"
    }
  }
}
